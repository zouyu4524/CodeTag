{
    "name": "Code Tag",
    "description": "This extension helps add html tag for editable selected context",
    "version": "0.2",
    "permissions": [
        "contextMenus"
    ],
    "content_scripts": [{
            "matches": ["<all_urls>"],
            "js": ["addTag.js"]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2
}